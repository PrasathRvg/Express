doctype html
html
   head
      title stuList
   body
      p <h1> Student Name List </h1>
      table(border="2" cellpadding="10" bgcolor="yellowgreen")
         thead(bgcolor="olivedrab")
            th &emsp;Names&emsp;       
         each val in details
            tr 
               td 
                  a(href=`/studentnamelist/${val._id}`) #{val.fullname}


